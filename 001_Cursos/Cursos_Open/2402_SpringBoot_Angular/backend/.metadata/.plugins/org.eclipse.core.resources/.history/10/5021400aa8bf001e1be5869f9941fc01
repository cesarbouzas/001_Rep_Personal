package es.cesar;


import java.sql.Date;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;

@Entity
public class Arduino {
		@Id
	 

	    private String ard_Id; // Cambiado a "ard_Id" para coincidir con el objeto JSON
	    private Date ard_Datetime;
	    private Float ard_Dist;
		public String getArd_Id() {
			return ard_Id;
		}
		public void setArd_Id(String ard_Id) {
			this.ard_Id = ard_Id;
		}
		public Date getArd_Datetime() {
			return ard_Datetime;
		}
		public void setArd_Datetime(Date ard_Datetime) {
			this.ard_Datetime = ard_Datetime;
		}
		public Float getArd_Dist() {
			return ard_Dist;
		}
		public void setArd_Dist(Float ard_Dist) {
			this.ard_Dist = ard_Dist;
		}
    


}